<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="SampleCARMavenProject"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modules>
    <module id="built_artifact" dir="built_artifact" name="built_artifact">
      <fileSets>
    	<fileSet filtered="true" encoding="UTF-8">
          <directory/>
          <includes>
            <include>*.xml</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory/>
          <includes>
            <include>**/*.xml</include>
          </includes>
        </fileSet>    
        <fileSet filtered="true" encoding="UTF-8">
          <directory/>
          <includes>
            <include>**/*.wsdl</include>
          </includes>
        </fileSet>    
      </fileSets>
    </module>
    <module id="__rootArtifactId__SimpleProxy" dir="__rootArtifactId__SimpleProxy" name="__rootArtifactId__SimpleProxy">
      <fileSets>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/synapse-config</directory>
          <includes>
            <include>**/*.xml</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory></directory>
          <includes>
            <include>artifact.xml</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="__rootArtifactId__" dir="__rootArtifactId__" name="__rootArtifactId__" />
  </modules>
</archetype-descriptor>
